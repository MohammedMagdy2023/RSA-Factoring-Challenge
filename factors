#!/usr/bin/python3
import sys
import signal

def factors(n):
    for i in range(2, int(n ** 0.5) + 1):
        if n % i == 0:
            return n // i, i
    return 1, n

def handler(signum, frame):
    raise Exception("Time is up!")

def main():
    signal.signal(signal.SIGALRM, handler)
    signal.alarm(5)

    try:    
        source = sys.argv[1]

        with open(source, "r") as f:
            for line in f:
                n = int(line.strip())
                p, q = factors(n)
                print("{:d}={:d}*{:d}".format(n, p, q))

    except Exception:
        print(Exception)
        exit(0)

if __name__ == '__main__':
    main()
